{
  "name": "@lachiejames/generate-project",
  "version": "1.0.2",
  "description": "CLI tool for generating projects with predefined templates",
  "author": "Lachie James",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "install:ci": "yarn install --frozen-lockfile",
    "lint:ci": "eslint .",
    "build": "tsc",
    "start": "ts-node .",
    "test": "jest --coverage --detectOpenHandles ./src",
    "teste2e": "jest --detectOpenHandles ./e2e",
    "format": "eslint . --fix; prettier . --write;"
  },
  "dependencies": {
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "inquirer": "8.2.0",
    "nunjucks": "3.2.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/fs-extra": "9.0.13",
    "@types/glob": "7.2.0",
    "@types/inquirer": "8.1.3",
    "@types/jest": "27.5.0",
    "@types/nunjucks": "3.2.0",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-unused-imports": "1.1.4",
    "eslint": "8.14.0",
    "jest": "28.0.3",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "ts-jest": "28.0.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.4"
  },
  "bin": {
    "generate-project": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/templates"
  ],
  "keywords": [
    "generate",
    "project",
    "typescript",
    "lambda",
    "docker"
  ],
  "publishConfig": {
    "access": "public"
  }
}
